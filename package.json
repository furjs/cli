{
	"name": "@furjs/cli",
	"version": "0.1.0-alpha",
	"description": "Fur.js CLI application",
	"author": "depthbomb",
	"keywords": [],
	"license": "GPLV3",
	"main": "./dist/index.js",
	"publishConfig": {
		"access": "public"
	},
	"files": [
		"dist",
		"!dist/**/*.js.map",
		"!dist/**/*.d.ts",
		"!dist/**/*.d.ts.map"
	],
	"engines": {
		"node": ">=16.0.0"
	},
	"bin": {
		"fur": "./dist/index.js"
	},
	"imports": {
		"#args": "./dist/args.js",
		"#utils": "./dist/utils.js",
		"#logger": "./dist/logger.js",
		"#tokens": "./dist/tokens.js",
		"#services/*": "./dist/services/*.js",
		"#commands/*": "./dist/commands/*.js",
		"#constants": "./dist/constants.js",
		"#interfaces": "./dist/interfaces/*.js"
	},
	"scripts": {
		"prebuild": "rimraf ./dist/*",
		"predist": "npm run prebuild",
		"dist": "swc ./src -d ./dist -s",
		"build": "tsc",
		"prerelease": "npm run dist",
		"release": "release-it"
	},
	"dependencies": {
		"axios": "^0.24.0",
		"commander": "^8.3.0",
		"didyoumean2": "^5.0.0",
		"execa": "^6.0.0",
		"glob": "^7.2.0",
		"lodash": "^4.17.21",
		"mri": "^1.2.0",
		"ms": "^2.1.3",
		"npmlog": "^6.0.0",
		"ora": "5",
		"pretty-bytes": "^5.6.0",
		"prompts": "^2.4.2",
		"reflect-metadata": "^0.1.13",
		"sanitize-filename": "^1.6.3",
		"source-map-support": "^0.5.21",
		"tslib": "^2.3.1",
		"tsyringe": "^4.6.0",
		"url-join": "^4.0.1"
	},
	"devDependencies": {
		"@swc/cli": "^0.1.55",
		"@swc/core": "^1.2.128",
		"@types/eslint": "^8.2.2",
		"@types/glob": "^7.2.0",
		"@types/lodash": "^4.14.178",
		"@types/ms": "^0.7.31",
		"@types/npmlog": "^4.1.4",
		"@types/prompts": "^2.4.0",
		"@types/source-map-support": "^0.5.4",
		"@types/url-join": "^4.0.1",
		"eslint": "^8.6.0",
		"release-it": "^14.12.1",
		"typescript": "^4.5.4"
	}
}
