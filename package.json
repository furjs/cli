{
	"name": "@furjs/cli",
	"version": "0.1.0-alpha.1",
	"description": "Fur.js CLI application",
	"author": "depthbomb",
	"keywords": [
		"@furjs/cli",
		"fur",
		"furjs",
		"cli",
		"furry",
		"typescript",
		"ts",
		"yarn"
	],
	"license": "GPLv3",
	"main": "./dist/index.js",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/furjs/cli.git"
	},
	"bugs": {
		"url": "https://github.com/furjs/cli/issues"
	},
	"publishConfig": {
		"access": "public"
	},
	"files": [
		"dist",
		"!dist/**/*.js.map",
		"!dist/**/*.d.ts",
		"!dist/**/*.d.ts.map"
	],
	"engines": {
		"node": ">=16.0.0"
	},
	"bin": {
		"fur": "./dist/index.js"
	},
	"imports": {
		"#args": "./dist/args.js",
		"#utils": "./dist/utils.js",
		"#logger": "./dist/logger.js",
		"#tokens": "./dist/tokens.js",
		"#services/*": "./dist/services/*.js",
		"#commands/*": "./dist/commands/*.js",
		"#constants": "./dist/constants.js",
		"#interfaces": "./dist/interfaces/*.js"
	},
	"scripts": {
		"prebuild": "rimraf ./dist/*",
		"build": "tsc",
		"predist": "npm run prebuild",
		"dist": "swc ./src -d ./dist -s",
		"prerelease": "npm run dist",
		"release": "release-it"
	},
	"dependencies": {
		"axios": "^0.24.0",
		"colorette": "^2.0.16",
		"commander": "^8.3.0",
		"glob": "^7.2.0",
		"ora": "5.4.1",
		"pretty-bytes": "^5.6.0",
		"prompts": "^2.4.2",
		"reflect-metadata": "^0.1.13",
		"sanitize-filename": "^1.6.3",
		"source-map-support": "^0.5.21",
		"tslib": "^2.3.1",
		"url-join": "^4.0.1"
	},
	"devDependencies": {
		"@swc/cli": "^0.1.55",
		"@swc/core": "^1.2.128",
		"@types/eslint": "^8.2.2",
		"@types/glob": "^7.2.0",
		"@types/prompts": "^2.4.0",
		"@types/source-map-support": "^0.5.4",
		"@types/url-join": "^4.0.1",
		"eslint": "^8.6.0",
		"release-it": "^14.12.1",
		"typescript": "^4.5.4"
	}
}
